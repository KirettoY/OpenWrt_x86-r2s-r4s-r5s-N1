
CONFIG_TARGET_x86=y
CONFIG_TARGET_x86_64=y
CONFIG_TARGET_ROOTFS_TARGZ=y
CONFIG_TARGET_KERNEL_PARTSIZE=256
CONFIG_TARGET_ROOTFS_PARTSIZE=2040

# EFI支持:
CONFIG_GRUB_IMAGES=y
CONFIG_GRUB_EFI_IMAGES=y
CONFIG_VMDK_IMAGES=n

# 其他需要安装的软件包:
CONFIG_PACKAGE_luci-ssl=n # uhttpd服务
CONFIG_PACKAGE_luci-ssl-nginx=y # nginx
CONFIG_PACKAGE_luci-app-lxc=y # nginx
CONFIG_PACKAGE_luci-app-diskman=y # nginx
CONFIG_PACKAGE_luci-app-openclash=y # nginx
CONFIG_PACKAGE_iwlwifi-firmware-ax210=y # nginx
#CONFIG_PACKAGE_kmod-r8168=n

CONFIG_OPENSSL_PREFER_CHACHA_OVER_GCM=n

CONFIG_KERNEL_CGROUPS=y
CONFIG_KERNEL_CGROUP_DEBUG=y
CONFIG_KERNEL_CGROUP_FREEZER=y
CONFIG_KERNEL_CGROUP_DEVICE=y
CONFIG_KERNEL_CGROUP_HUGETLB=y
CONFIG_KERNEL_CGROUP_PIDS=y
CONFIG_KERNEL_CGROUP_RDMA=y
CONFIG_KERNEL_CGROUP_BPF=y
CONFIG_KERNEL_CGROUP_CPUACCT=y
CONFIG_KERNEL_CGROUP_SCHED=y
